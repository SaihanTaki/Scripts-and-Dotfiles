{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "macos": "mac"
          },
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "foreground": "#26C6DA",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .UserName }}: ",
          "foreground": "#26C6DA",
          "type": "session",
          "style": "plain"
        },
        {
          "foreground": "#0CA0D8",
          "properties": {
            "folder_separator_icon": "/",
            "style": "mixed"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#14A5AE",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[{{ .Venv }} {{ .Major }}.{{ .Minor }}]",
          "foreground": "#906cff",
          "type": "python",
          "style": "powerline"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "âžœ ",
          "foreground": "#26C6DA",
          "type": "text",
          "style": "plain"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}

